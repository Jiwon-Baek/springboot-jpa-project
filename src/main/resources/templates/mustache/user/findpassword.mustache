{{>mustache/layout/header}}


<div class="wrapper">
    <div class="main">
        <span class="title2">비밀번호 찾기</span>
        <form>
            <div class="box">
                <div class="input">
                    <input type="text" id="username" placeholder="아이디" autofocus/>
                </div>
                    <div class="input">
                        <input type="text" id="name" placeholder="이름" autofocus/>
                    </div>
                    <div class="input">
                        <input type="text" id="email" placeholder="이메일" autofocus/>
                    </div>
                    <div class="button">
                        <button type="button" class="btn btn-primary" id="btn-findPassword">비밀번호 찾기</button>
                    </div>
                    <div class="notice" id="findpassword"></div>
                    <div class="last">
                        <div class="left">
                            <a class="signup " href="/signup">회원가입</a>
                        </div>
                        <div class="middle">
                            <a href="/findid">아이디 찾기</a>
                        </div>
                        <div class="right">
                            <a href="/login">로그인</a>
                        </div>
                    </div>
                </div>
        </form>
    </div>
</div>

<link href="/app/css/Login-find.css" rel="stylesheet" type="text/css">

{{>mustache/layout/footer}}

